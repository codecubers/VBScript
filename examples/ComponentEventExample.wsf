<job id=".wsf usage demo for ComponentEventExample.wsc">
    <object
        progid="VBScripting.EventExample"
        id="vee"
        events="true"/>
    <script language="VBScript">
        vee.FireTestEvent
        Sub vee_TestEvent
            MsgBox "Event fired successfully.", vbInformation, WScript.ScriptName
        End Sub
    </script>
</job>
